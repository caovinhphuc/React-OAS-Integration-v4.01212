{
  "name": "react-oas-integration",
  "version": "3.0.0",
  "description": "React OAS Integration v3.0 - AI-Powered Platform",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "install:source-map-explorer": "npm install --save source-map-explorer",
    "test": "react-scripts test",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "GENERATE_SOURCEMAP=true react-scripts build && npm run analyze",
    "serve:build": "npx serve -s build -p 3000",
    "serve:deployed": "bash serve-build.sh 3000",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"npm run backend\" \"npm run ai-service\"",
    "backend": "cd backend && npm start",
    "ai-service": "cd ai-service && python -m uvicorn main_simple:app --host 0.0.0.0 --port 8000 --reload",
    "analyze": "source-map-explorer build/static/js/main.*js",
    "vercel-build": "npm run build",
    "analyze-deps": "node scripts/analyze-bundle-deps.js",
    "perf:bundle": "node scripts/perf-bundle-analyzer.js",
    "perf:deps": "npx depcheck",
    "perf:size": "npx size-limit",
    "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:check": "npx eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 || echo 'Lint warnings found'",
    "lint:fix": "npx eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "npx prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test:frontend": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci",
    "test:backend": "jest",
    "test:integration": "jest integration-tests",
    "test:e2e": "jest end-to-end-tests",
    "test:all": "concurrently \"npm run test:frontend\" \"npm run test:backend\" \"npm run test:integration\" \"npm run test:e2e\"",
    "test:google-sheets": "node scripts/testGoogleSheets.js",
    "test:api": "node scripts/test-api-endpoints.js",
    "test:automation": "node scripts/test-automation-system.js",
    "test:websocket": "node scripts/test-websocket.js",
    "test:complete": "node scripts/test-all.js",
    "test:scripts": "node scripts/test-all.js",
    "fix:ports": "bash scripts/fix-port-conflict.sh",
    "kill:port": "bash scripts/kill-port.sh",
    "health:full": "bash scripts/full-health-check.sh",
    "check:ports": "bash scripts/check-ports.sh",
    "check:backend": "bash scripts/check-backend.sh",
    "verify:setup": "bash scripts/verify-setup.sh",
    "cleanup:duplicates": "bash scripts/cleanup-duplicates.sh",
    "fix:chunks": "bash scripts/fix-chunk-errors.sh",
    "update:vercel": "bash scripts/update_vercel_env.sh",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer build/static/js/main.*js",
    "webpack-bundle-analyzer-production": "webpack-bundle-analyzer build/static/js/main.*js --mode production",
    "webpack-bundle-analyzer-deps": "webpack-bundle-analyzer build/static/js/main.*js --mode development"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@sendgrid/mail": "^8.1.6",
    "antd": "^5.29.3",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.2",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.5",
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "handlebars": "^4.7.8",
    "http-proxy-middleware": "^3.0.5",
    "mjml": "^4.18.0",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^7.0.11",
    "postcss-loader": "^8.2.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-redux": "^8.1.3",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.4",
    "redux": "^5.0.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^10.0.0",
    "browserify-zlib": "^0.2.0",
    "concurrently": "^8.2.2",
    "crypto-browserify": "^3.12.1",
    "eslint-config-react-app": "^7.0.1",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "source-map-loader": "^5.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.16",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "oas",
    "ai",
    "analytics",
    "dashboard"
  ],
  "author": "React OAS Team",
  "license": "MIT"
}
