{
  "name": "mia-vn-google-integration",
  "version": "1.0.0",
  "private": true,
  "description": "MIA VN Google Integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run backend\" \"npm run ai-service\"",
    "backend": "cd backend && npm start",
    "ai-service": "cd ai-service && python -m uvicorn main_simple:app --host 0.0.0.0 --port 8000 --reload",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "install:dependencies:core": "npm install && npm install --save source-map-explorer && npm install --save-dev eslint eslint-config-prettier eslint-plugin-prettier prettier source-map-explorer size-limit tsc typescript nth-check postcss webpack-dev-server husky lint-staged",
    "install:dependencies:dev": "npm install --save-dev eslint eslint-config-prettier eslint-plugin-prettier prettier source-map-explorer size-limit tsc typescript nth-check postcss webpack-dev-server husky lint-staged",
    "install:dependencies:prod": "npm install --save source-map-explorer",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type:check": "tsc --noEmit || echo 'TypeScript check skipped'",
    "type:watch": "tsc --noEmit --watch || echo 'TypeScript watch skipped'",
    "pre-commit": "lint-staged",
    "validate": "npm run lint:check && npm run format:check && npm run test:ci",
    "validate:full": "npm run validate && npm run build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "GENERATE_SOURCEMAP=true react-scripts build && npm run analyze",
    "serve:build": "npx serve -s build -p 3000",
    "serve:deployed": "bash serve-build.sh 3000",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*js",
    "vercel-build": "npm run build",
    "analyze-deps": "node scripts/analyze-bundle-deps.js",
    "perf:bundle": "node scripts/perf-bundle-analyzer.js",
    "perf:deps": "npx depcheck",
    "perf:size": "npx size-limit",
    "depcheck": "npx depcheck",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer build/static/js/main.*js",
    "webpack-bundle-analyzer-production": "webpack-bundle-analyzer build/static/js/main.*js --mode production",
    "docs:generate": "node scripts/utils/generate-complete-docs.js",
    "docs:watch": "./scripts/utils/auto-update-docs.sh true",
    "test:scripts": "node scripts/test-all.js",
    "fix:scripts": "./scripts/utils/fix-script-paths.sh",
    "fix:git-push": "./scripts/utils/fix-git-push.sh",
    "check:ports": "bash scripts/check-ports.sh",
    "check:backend": "bash scripts/check-backend.sh",
    "verify:setup": "bash scripts/verify-setup.sh",
    "cleanup:duplicates": "bash scripts/cleanup-duplicates.sh",
    "vercel-build": "GENERATE_SOURCEMAP=false CI=false react-scripts build",
    "fix:chunks": "bash scripts/fix-chunk-errors.sh",
    "update:vercel": "bash scripts/update_vercel_env.sh",
        "health-check": "node scripts/health-check.js",
    "kill:port": "bash scripts/kill-port.sh",
    "fix:ports": "bash scripts/fix-port-conflict.sh",
    "health:full": "./scripts/utils/comprehensive-health-check.sh",
    "health:quick": "curl -f http://localhost:3001/health && curl -f http://localhost:8000/health && echo '✅ All services healthy' || echo '❌ Some services down'"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.4",
    "size-limit": "^8.1.0",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "vercel-build": "GENERATE_SOURCEMAP=false CI=false react-scripts build",
    "analyze-deps": "node scripts/analyze-bundle-deps.js",
    "perf:bundle": "node scripts/perf-bundle-analyzer.js",
    "perf:deps": "npx depcheck",
    "perf:size": "npx size-limit",
    "depcheck": "npx depcheck",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer build/static/js/main.*js",
    "webpack-bundle-analyzer-production": "webpack-bundle-analyzer build/static/js/main.*js --mode production",
    "docs:generate": "node scripts/utils/generate-complete-docs.js",
    "docs:watch": "./scripts/utils/auto-update-docs.sh true",
    "test:scripts": "node scripts/test-all.js",
    "fix:scripts": "./scripts/utils/fix-script-paths.sh",
    "fix:git-push": "./scripts/utils/fix-git-push.sh",
    "check:ports": "bash scripts/check-ports.sh",
    "check:backend": "bash scripts/check-backend.sh",
    "verify:setup": "bash scripts/verify-setup.sh",
    "cleanup:duplicates": "bash scripts/cleanup-duplicates.sh",
    "fix:chunks": "bash scripts/fix-chunk-errors.sh",
    "update:vercel": "bash scripts/update_vercel_env.sh",
    "health-check": "node scripts/health-check.js",
    "kill:port": "bash scripts/kill-port.sh",
    "fix:ports": "bash scripts/fix-port-conflict.sh",
    "health:full": "./scripts/utils/comprehensive-health-check.sh",
    "health:quick": "curl -f http://localhost:3001/health && curl -f http://localhost:8000/health && echo '✅ All services healthy' || echo '❌ Some services down'"
          },
  "devDependencies": {
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.4",
    "size-limit": "^8.1.0",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.35",
    "webpack-dev-server": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.35",
    "webpack-dev-server": "^5.0.0"
  },
  "resolutions": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.35",
    "webpack-dev-server": "^5.0.0"
  }
}
