{
  "name": "mia-logistics-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:check": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules node_modules/.vite && npm cache clean --force",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "health": "npm run type-check && npm run lint:check",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^2.0.2",
    "@tanstack/react-query": "^5.90.12",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-spreadsheet": "^5.0.2",
    "jwt-decode": "^4.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bull": "^3.15.9",
    "@types/node": "^25.0.3",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.3",
    "baseline-browser-mapping": "^2.9.7",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "sharp": "^0.34.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.2.0"
  }
}
